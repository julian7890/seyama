generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model schedule {
  id   String  @id @default(auto()) @map("_id") @db.ObjectId
  date String
  en   Json?
  jp   Json?
  url  String?
}

model profile {
  id     String @id @default(auto()) @map("_id") @db.ObjectId
  jpData String
  enData String
}

model news {
  id    String  @id @default(auto()) @map("_id") @db.ObjectId
  date  String
  en    Json?
  jp    Json?
  url   String?
  image String?
}
